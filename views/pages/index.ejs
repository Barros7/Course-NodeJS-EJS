<!DOCTYPE html>

    <%- include('../layout/head'); %>

    <body class="container">

        <%- include('../layout/header', {page:'Home'}); %>

        <main>
            <div class="jumbotron">
                <h1><%= title %></h1>
                <p><%= paragraph %></p>
            </div>
            <ul>
                <% qualitys.forEach((quality) => { %>
                    <li>
                        <strong><%= quality.title %></strong> <%= quality.message %>
                    </li>
                <% }) %>
            </ul>
        </main>

        <%- include('../layout/footer'); %>

    </body>
</html>